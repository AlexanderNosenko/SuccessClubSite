<% if team.any? %>
	<% @team.each do |user| %>
		<tr>
			<td class="client-avatar"><img alt="image" src="<%= image_path('a2.jpg') %>"> </td>
			<td><a data-toggle="tab" href="#<%= user.id %>" class="client-link"><%= user.name %>&nbsp;<%= user.last_name %></a></td>
			<td>Пользователь</td>
			<td><%= user.created_at %></td>
			<!-- <td class="client-status"><span class="label label-primary">Online</span></td> -->
			<!-- <td class="contact-type"><i class="fa fa-envelope" style="font-size: 20px"></i></td> -->
		</tr>
	<% end %>
<% else %>
	<tr> В вашей команде пока никого нет..</tr>
<% end %>
