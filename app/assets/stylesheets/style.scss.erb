/* font-family: "RobotoRegular"; */
@font-face {
	font-family: "RobotoRegular";
	src: url("<%= font_path 'RobotoRegular/RobotoRegular.eot' %>");
	src: url("<%= font_path 'RobotoRegular/RobotoRegular.eot' %>?#iefix")format("embedded-opentype"),
	url("<%= font_path 'RobotoRegular/RobotoRegular.woff' %>") format("woff"),
	url("<%= font_path 'RobotoRegular/RobotoRegular.ttf' %>") format("truetype");
	font-style: normal;
	font-weight: normal;
}
